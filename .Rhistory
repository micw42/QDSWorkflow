normal_deseq = function(full_df, cond_df) {
dds <- DESeqDataSetFromMatrix(countData = full_df, colData = cond_df, design = ~ 1)
vsd <- DESeq2::varianceStabilizingTransformation(dds)
vsd = as.data.frame(assay(vsd))
return(vsd)
}
