make_grouped_hist = function(pred_df, ann_df, grouping1, xvar="QDS", grouping2=NULL,
byvar=c("sample_id"="sample_id"), title=NULL, levels=NULL) {
pred_df = inner_join(pred_df, ann_df, by=byvar)
if (!is.null(levels)) {
pred_df[[grouping1]] = factor(pred_df[[grouping1]], ordered=T, levels=levels)
}
if (is.null(grouping2)) {
p = ggplot(pred_df, aes(x=!!sym(xvar), group=!!sym(grouping1), color=!!sym(grouping1), fill=!!sym(grouping1)))+
geom_density(adjust=1.5, alpha=.4) +
ggtitle(title)
return(p)
}
modes_df = pred_df %>% group_by(!!sym(grouping1), !!sym(grouping2)) %>%
summarise(QDS_mode=get_modes(!!sym(xvar)))
print(modes_df)
p = ggplot(pred_df, aes(x=!!sym(xvar), fill=NULL, group=!!sym(grouping2), color=!!sym(grouping2)))+
scale_color_brewer(palette="Dark2")+
geom_density(adjust=1.5, alpha=.4) +
geom_vline(data=modes_df, mapping=aes(xintercept=QDS_mode)) +
facet_wrap(as.formula(paste("~", grouping1)), ncol=1) +
ggtitle(title)
return(p)
}
make_grouped_hist(df, nsc_ann, grouping1 = "celltype", xvar="QDS", grouping2="age")
